{
    "sourceMaps": "inline",
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false,
          "useBuiltIns": "entry",
          "corejs": { "version": 3, "proposals": true }
        }
      ],
      "@babel/preset-react",
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ]
    ]
  }
  