{
  "name": "react-storybook-library",
  "version": "1.0.7",
  "description": "Template for a React TypeScript npm package",
  "author": "Mykhailo Hudzevatyi @michaelgudzevskyi",
  "private": false,
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open  --config webpack.config.dev.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu NODE_ENV=test jest --verbose",
    "coverage": "npm test -- --verbose=false --coverage --silent --noStackTrace",
    "build": "webpack --config webpack.config.prod.js",
    "build-storybook": "build-storybook -c .storybook -o storybook-static",
    "watch": "tsc -w",
    "clean": "gh-pages-clean",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "react": "^17.0.1"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-a11y": "^6.2.5",
    "@storybook/addon-actions": "^6.2.5",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.2.5",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.5",
    "@storybook/addon-links": "^6.2.5",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storyshots": "^6.2.5",
    "@storybook/addon-storysource": "^6.2.5",
    "@storybook/addon-viewport": "^6.2.5",
    "@storybook/addons": "^6.2.5",
    "@storybook/react": "^6.2.5",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.1.1",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-prettier-react": "0.0.24",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.4.0",
    "full-icu": "^1.3.4",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^1.19.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.2",
    "semver": "^7.3.5",
    "storybook-addon-designs": "^5.4.5",
    "styled-components": "^5.2.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "homepage": "http://michaelgudzevskyi.github.io/react-storybook-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelgudzevskyi/react-storybook-library.git"
  },
  "bugs": {
    "url": "https://github.com/michaelgudzevskyi/react-storybook-library/issues",
    "email": "hello@michaelgudzevskyi.com"
  },
  "keywords": [
    "storybook",
    "react",
    "reactjs",
    "typescript",
    "ui-components",
    "library",
    "npm",
    "npm-package"
  ]
}
